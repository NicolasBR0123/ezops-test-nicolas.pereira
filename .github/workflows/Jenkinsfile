
pipeline {
     agent any
     environment {
     DOCKERHUB_CREDENTIALS = credentials('docker-hub-nicolas')
     }

    stages {   
     	stage('Build docker image') {
       	    steps{
                   docker{
                    dockerapp = docker.build ("webchat:nodealpine:18.10"  '-t ./app')
                   }
     	    }
     	}     
    }
}	
