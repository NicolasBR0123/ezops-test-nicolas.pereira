
pipeline {
     agent any
     environment {
     DOCKERHUB_CREDENTIALS = credentials('docker-hub-nicolas')
     }

    stages {   
     	stage('Build docker image') {
       	    steps{
                   scipt{
                    sh 'docker build webchat:nodealpine18.0 -t ./app'
                   }
     	    }
     	}     
    }
}	
